<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisdemo.repository.TeacherRepository">
    <resultMap id="rmteacher1" type="com.example.mybatisdemo.entity.Teacher">
        <id column="teacherid" property="teacherid"></id>
        <collection property="userList" select="com.example.mybatisdemo.repository.UserRepository.seleteById"
        column="teacherid">
        </collection>
    </resultMap>
    <select id="findall1" resultMap="rmteacher1">
        select * from teacher
    </select>

    <select id="selectById" resultType="Teacher">
        select * from teacher where teacherid=#{teacherid}
    </select>
</mapper>